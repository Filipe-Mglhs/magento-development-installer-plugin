{
    "name": "filipemglhs/magento-development-installer-plugin",
    "description": "Composer Custom Installer for Magento 2",
    "type": "composer-plugin",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "filipe",
            "email": "mglhs.filipe@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "1.6.* || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "FilipeMglhs\\MagentoDevelopmentInstallerPlugin\\": "src/"
        }
    },
    "extra": {
        "class": "FilipeMglhs\\MagentoDevelopmentInstallerPlugin\\Composer\\Plugin"
    }
}
